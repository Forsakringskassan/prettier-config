{
  "name": "@forsakringskassan/prettier-config",
  "version": "2.0.1",
  "description": "FK Prettier Config",
  "keywords": [
    "prettier"
  ],
  "homepage": "https://github.com/Forsakringskassan/prettier-config",
  "bugs": "https://github.com/Forsakringskassan/prettier-config/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/Forsakringskassan/prettier-config.git"
  },
  "license": "MIT",
  "author": "Försäkringskassan",
  "main": "index.js",
  "bin": {
    "prettier": "prettier.cjs"
  },
  "files": [
    "index.js",
    "prettier.cjs"
  ],
  "scripts": {
    "eslint": "eslint .",
    "eslint:fix": "eslint --fix .",
    "lint": "eslint .",
    "prepare": "husky",
    "prettier": "prettier .",
    "prettier:check": "npm run prettier -- --check",
    "prettier:write": "npm run prettier -- --write",
    "test": "npm run eslint && npm run prettier:check",
    "posttest": "node prettier.cjs --version"
  },
  "commitlint": {
    "extends": "@forsakringskassan/commitlint-config/no-jira"
  },
  "lint-staged": {
    "*.{js,json,md,css,scss,html,vue,ts}": "prettier --write",
    "*.{js,vue,ts}": "eslint --fix"
  },
  "prettier": "./index.js",
  "dependencies": {
    "prettier": "3.2.5"
  },
  "devDependencies": {
    "@forsakringskassan/commitlint-config": "1.3.0",
    "@forsakringskassan/eslint-config": "11.0.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "npm-pkg-lint": "2.4.0",
    "ts-node": "10.9.2"
  },
  "engines": {
    "node": ">= 16",
    "npm": ">= 7"
  },
  "release": {
    "extends": "@forsakringskassan/semantic-release-config"
  }
}
