{
  "name": "@forsakringskassan/prettier-config",
  "version": "1.3.11",
  "description": "FK Prettier Config",
  "keywords": [
    "prettier"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Forsakringskassan/prettier-config.git"
  },
  "license": "MIT",
  "author": "Försäkringskassan",
  "main": "index.js",
  "bin": {
    "prettier": "prettier.js"
  },
  "files": [
    "index.js",
    "prettier.js"
  ],
  "scripts": {
    "eslint": "eslint .",
    "eslint:fix": "eslint --fix .",
    "lint": "eslint .",
    "prepare": "husky install",
    "prettier": "prettier .",
    "prettier:check": "npm run prettier -- --check",
    "prettier:write": "npm run prettier -- --write",
    "test": "npm run lint && npm run prettier:check"
  },
  "commitlint": {
    "extends": "@forsakringskassan/commitlint-config"
  },
  "lint-staged": {
    "*.{js,json,md,css,scss,html,vue,ts}": "prettier --write",
    "*.{js,vue,ts}": "eslint --fix"
  },
  "prettier": "./index.js",
  "dependencies": {
    "prettier": "2.8.8"
  },
  "devDependencies": {
    "@forsakringskassan/commitlint-config": "0.9.6",
    "@forsakringskassan/eslint-config": "9.7.13",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "ts-node": "10.9.2"
  },
  "peerDependencies": {
    "prettier": "^2"
  },
  "engines": {
    "node": ">= 14",
    "npm": ">= 7"
  },
  "release": {
    "extends": "@forsakringskassan/semantic-release-config"
  },
  "renovate": {
    "extends": [
      "local>npm-packages/renovate-config-fk:lib"
    ],
    "packageRules": [
      {
        "matchPackageNames": [
          "prettier"
        ],
        "matchDepTypes": [
          "dependencies"
        ],
        "rangeStrategy": "pin"
      }
    ]
  }
}
